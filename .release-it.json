{
  "git": {
    "commit": true,
    "commitMessage": "[github-bot] released version ${version}",
    "tag": true,
    "tagName": "${version}",
    "requireCleanWorkingDir": true
  },
  "npm": {
    "publish": true
  },
  "github": {
    "release": true
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": true
    },
    "npm": false
  }
}
