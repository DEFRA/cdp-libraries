{
  "$schema": "https://unpkg.com/@changesets/config@3.1.1/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    { "repo": "defra/cdp-libraries" }
  ],
  "commit": true,
  "fixed": [],
  "linked": [],
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "minor",
  "ignore": [],
  "release-it": {
    "github": {
      "release": true,
      "releaseNotes": "cat CHANGELOG.md | sed -n '/^## /{n; :a; /^## /q; p; n; ba}'"
    }
  }
}
